{
  "name": "nerthusaddon",
  "version": "3.1.0",
  "description": "Addon for nerthus server in game [margonem](http://www.margonem.pl/)",
  "main": "/src/main.js",
  "directories": {
    "test": "test"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "firefox 52"
  ],
  "dependencies": {
    "jquery": "3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "babel-loader": "^8.1.0",
    "cheerio": "^1.0.0-rc.3",
    "coveralls": "^3.1.0",
    "eslint": "^7.11.0",
    "expect.js": "^0.3.1",
    "inject-loader": "^4.0.1",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "sinon": "^9.2.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-node-externals": "^2.5.2"
  },
  "scripts": {
    "test": "webpack --config ./test/webpack-test.config.js --entry ./test/main.js --output-path ./test-dist && mocha ./test-dist/main.js && rm -rf ./test-dist",
    "test-leaks": "webpack --config ./test/webpack-test.config.js ./test/main.js --entry ./test/main.js --output-path ./test-dist && mocha --check-leaks ./test-dist/main.js && rm -rf ./test-dist",
    "test-coverage": "webpack --config ./test/webpack-test.config.js --entry ./test/main.js --output-path ./test-dist && nyc --reporter=text --reporter=text-lcov mocha ./test-dist/main.js && rm -rf ./test-dist",
    "test-code-quality": "eslint src/*",
    "coverage-raport-text": "nyc report --reporter=text",
    "coverage-raport-text-lcov": "nyc report --reporter=text-lcov",
    "push-coverage-raport": "nyc report --reporter=text-lcov | coveralls",
    "clean-coverage": "rm -rf ./coverage ./.nyc_output",
    "build": "npx webpack",
    "update-version": "git rev-parse --short HEAD > version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nerthus-margonem/nerthusaddon.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nerthus-margonem/nerthusaddon/issues"
  },
  "homepage": "https://github.com/nerthus-margonem/nerthusaddon#readme"
}
