{
  "name": "nerthusaddon",
  "version": "3.1.0",
  "description": "Addon for nerthus server in game [margonem](http://www.margonem.pl/)",
  "main": "/src/main.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "jquery": "3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "cheerio": "^1.0.0-rc.3",
    "coveralls": "^3.1.0",
    "eslint": "^6.8.0",
    "expect.js": "^0.3.1",
    "inject-loader": "^4.0.1",
    "mocha": "^7.1.2",
    "mocha-webpack": "2.0.0-beta.0",
    "nyc": "^15.0.1",
    "sinon": "^9.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^4.0.0-beta.8",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "test": "mocha-webpack --webpack-config test/webpack-test.config.js ./test/main.js",
    "test-leaks": "mocha-webpack --check-leaks --webpack-config test/webpack-test.config.js ./test/main.js",
    "test-coverage": "nyc --reporter=text --reporter=text-lcov mocha-webpack --webpack-config test/webpack-test.config.js ./test/main.js",
    "test-code-quality": "eslint src/*",
    "coverage-raport-text": "nyc report --reporter=text",
    "coverage-raport-text-lcov": "nyc report --reporter=text-lcov",
    "push-coverage-raport": "nyc report --reporter=text-lcov | coveralls",
    "clean-coverage": "rm -rf ./coverage ./.nyc_output",
    "build": "npx webpack",
    "update-version": "git rev-parse --short HEAD > version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akrzyz/nerthusaddon.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/akrzyz/nerthusaddon/issues"
  },
  "homepage": "https://github.com/akrzyz/nerthusaddon#readme"
}
